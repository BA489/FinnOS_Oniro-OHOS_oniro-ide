{
  "private": true,
  "name": "oniro-ide-browser",
  "version": "0.0.0",
  "dependencies": {
    "@theia/core": "1.35.0",
    "@theia/editor": "1.35.0",
    "@theia/filesystem": "1.35.0",
    "@theia/markers": "1.35.0",
    "@theia/messages": "1.35.0",
    "@theia/monaco": "1.35.0",
    "@theia/navigator": "1.35.0",
    "@theia/preferences": "1.35.0",
    "@theia/process": "1.35.0",
    "@theia/terminal": "1.35.0",
    "@theia/workspace": "1.35.0",
    "oniro-ide-extension": "0.0.0"
  },
  "devDependencies": {
    "@theia/cli": "1.35.0"
  },
  "scripts": {
    "build": "theia build --mode development",
    "start": "theia start",
    "watch": "concurrently -c blue,yellow -n tsc,webpack \"yarn watch:compile\" \"yarn watch:bundle\"",
    "watch:compile": "lerna run watch --scope=oniro-ide-extension",
    "watch:bundle": "theia build --watch --mode development",
    "rebuild": "theia rebuild:browser --cacheRoot ../.."
  },
  "theia": {
    "target": "browser"
  }
}
